{
    "name": "electron-utility",
    "version": "0.0.1",
    "description": "Native Node module which provides various utility functions on different platforms",
    "main": "index.js",
    "gypfile": true,
    "scripts": {
        "test-node": "tsc -p test-node && node test-node/index.js",
        "test-electron": "tsc -p test-electron && node test-electron/index.js",
        "build-dev": "node-gyp configure rebuild --debug",
        "build": "node-gyp configure rebuild",
        "build-electron": "node-gyp configure rebuild --dist-url=https://atom.io/download/electron --target=v3.0.0",
        "postinstall": "node scripts/postinstall"
    },
    "author": "Ante Burazer",
    "license": "ISC"
  }
